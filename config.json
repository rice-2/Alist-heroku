{
    "force": false,
    "address": "0.0.0.0",
    "port": 8080,
    "assets": "https://npm.elemecdn.com/alist-web@$version/dist",
    "database": {
      "type": "${DB_TYPE}",
      "host": "${DB_HOST}",
      "port": "${DB_PORT}",
      "user": "${DB_USER}",
      "password": "${DB_PASS}",
      "name": "${DB_NAME}",
      "db_file": "data/data.db",
      "table_prefix": "${DB_TABLE_PREFIX}",
      "ssl_mode": "disable"
    },
    "scheme": {
      "https": false,
      "cert_file": "",
      "key_file": ""
    },
    "cache": {
      "expiration": 60,
      "cleanup_interval": 120
    },
    "temp_dir": "data/temp"
  }